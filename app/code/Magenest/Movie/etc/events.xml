<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="adminhtml_customer_prepare_save">
        <observer name="customer_save"
                  instance="Magenest\Movie\Observer\RegisterVisit" />
    </event>
<!--    This event catch movie save after-->
    <event name="adminhtml_movie_save_after">
        <observer name="movie_save"
                  instance="Magenest\Movie\Observer\ObserveMovie" />
    </event>
</config>