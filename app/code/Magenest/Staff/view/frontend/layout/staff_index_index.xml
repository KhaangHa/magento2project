<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd"
      label="Be a Staff" design_abstraction="custom">
    <update handle="customer_account"/>
    <head>
        <title>Join the team</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Edit you profile</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magenest\Staff\Block\StaffForm" name="staff-form" template="Magenest_Staff::staff_form.phtml"/>
        </referenceContainer>
    </body>
</page>